      "role":  "Developer"
    },
    {
      "name":     "eos new media GmbH & Co. KG",
      "email":    "info@eosnewmedia.de",
      "homepage": "http://www.eosnewmedia.de"
    }
  ],
  "minimum-stability": "stable",
